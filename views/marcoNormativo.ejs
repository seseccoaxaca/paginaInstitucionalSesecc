        <table id="users" class="table table-bordered table-striped text-center mt-4 color">
            <thead>
                <tr class="color-tabla2 text-white">
                    <!-- <th scope="col">ID</th> -->
                    <th scope="col">LEY</th>
                    <th scope="col">TÍTULO DE LA LEY</th>
                    <th scope="col">PUBLICACIÓN</th>
                    <th scope="col">ÚLTIMA REFORMA</th>
                    <th scope="col">PDF</th>
                    <th scope="col">WORD</th>
                </tr>
            </thead>
            <tbody>
                <% results.forEach((user)=>{ %>
                    <div>
                        <% if(user) { %>
                            <tr>

                                <td class="align-middle">
                                    <%= user.tipo %>
                                </td>

                                <td class="align-middle">
                                    <%= user.descripcion%>
                                </td>
                                <td class="align-middle">
                                    <%= user.publicacion %>
                                </td>
                                <td class="align-middle">
                                    <%= user.reforma %>
                                </td class="align-middle">
                                <td>
                                    <a href="<%= user.ruta %>/<%= user.pdf %>" data-bs-toggle="tooltip" data-bs-placement="bottom" title="<%= user.pdf %>"><i class="bi bi-file-earmark-word h2 icon-pdf" ></i></a>

                                    </a>

                                </td>
                                <td>
                                    <a href="<%= user.ruta %>/<%= user.word %>" data-bs-toggle="tooltip" data-bs-placement="bottom" title="<%= user.word %>"><i class="bi bi-file-earmark-word h2 icon-word" ></i></a>

                                    </a>

                                </td>
                            </tr>
                            <% } %>
                    </div>


                    <% }) %>

            </tbody>
        </table>
