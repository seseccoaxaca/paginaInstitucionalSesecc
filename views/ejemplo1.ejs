    <div class="colorBG5">
        <div class=" py-3">
            <br><br>

            <div class="container-fluid" style="max-width: 1000px;">
                <div class="card text-dark bg-light mt-3">
                    <div class=" text-center card-header h3 color-titulo1">Noticias</div>
                </div>
                <div class="row g-0">
                    <% for(let i = l-1 ; i > l-4 ; i--)  { %>
                        <div class="col-md-4 ">
                            <div class="card m-3 ">
                                <div>
                                    <div class=" text-center my-0 p-2">
                                        <img src="<%= results[i].ruta %>/<%= results[i].archivo %>" class="img-fluid rounded" alt="img" style="min-width: 100%;">
                                    </div>
                                    <div class="p-0 m-0">

                                        <div class="pt-2 px-2" style="min-height: 200px;">
                                            <h6 class="card-title">
                                                <%= results[i].titulo %>
                                            </h6>
                                            <p class="card-text small">
                                                <%= results[i].contenido %>
                                            </p>



                                        </div>

                                        <div class=" my-0 mx-3">
                                            <div class="mb-1">
                                                <a href="<%= results[i].link %>" class="btn btn-outline-secondary float-end">
                                                    <p class="card-text">Leer mas...</p>
                                                </a>
                                            </div>
                                            <br>
                                            <hr class=" mb-0">
                                            <p class="card-text align-bottom mb-0 text-end"><small id="fecha<%= [i] %>" class="text-muted"></small></p>
                                        </div>

                                    </div>
                                </div>
                            </div>

                            <script>
                                var fecha = "<%= results[i].fecha %>",
                                    year = fecha.substring(0, fecha.indexOf("-")),
                                    month = fecha.substring(5, fecha.lastIndexOf("-")),
                                    day = fecha.substring(fecha.lastIndexOf("-") + 1);
                                console.log(fecha)
                                var pfecha = document.getElementById('fecha<%= [i] %>');

                                var mes = ['enero', 'febrero', 'marzo', 'abril', 'mayo', 'junio', 'julio', 'agosto', 'septiembre', 'octubre', 'noviembre', 'diciembre'];
                                pfecha.textContent = day + " de " + mes[month - 1] + " del " + year;
                            </script>
                        </div>
                        <% } %>
                </div>
                <div class="card text-center mx-3">

                    <div class="card-body">
                        <!-- <h5 class="card-title">Noticias anteriores</h5> -->
                        <!-- <p class="card-text">Noticias anteriores</p> -->
                        <a href="noticias2" class="btn text-white" style=" background-color: #46AAA6; ">Ver todas las publicaciones</a>
                    </div>

                </div>
            </div>

        </div>
    </div>
